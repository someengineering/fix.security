    "autoprefixer": "10.4.19",
    "encoding": "0.1.13",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-simple-import-sort": "12.0.0",
    "eslint-plugin-unused-imports": "3.1.0",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "next-sitemap": "4.2.3",
    "postcss": "8.4.38",
