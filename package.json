    "feed": "4.2.2",
    "github-slugger": "2.0.0",
    "graphql": "16.8.1",
    "graphql-request": "6.1.0",
    "lodash": "4.17.21",
    "next": "14.2.0",
    "next-client-cookies": "1.1.1",
    "next-plausible": "3.12.0",
    "next-share": "0.27.0",
    "posthog-js": "1.121.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
